A,cAAvB,IAAuB;AAChC,UAAA,KAAI,GAAI,KAAI,GAAI,IAAI,CAAC,QAAL,GAAgB,IAAI,CAAC,MAArC;AACF;AAJe;AAAA;AAAA;AAAA;AAAA;;AAKf,aAAO,KAAP;AACF;AAPQ,GAZG;AAqBb,EAAA,OAAO,EAAE;AACP,IAAA,UADO,sBACI,IADJ,EACU;AACf,UAAI,IAAI,CAAC,UAAL,IAAmB,KAAvB,EAA8B;AAC5B,aAAK,YAAL,CAAkB,OAAlB,CAA0B,IAA1B;AACA,QAAA,IAAI,CAAC,QAAL;AACF,OAHA,MAGO;AACL,YAAI,gBAAe,GAAI,KAAK,YAAL,CAAkB,SAAlB,CAA4B,UAAC,WAAD;AAAA,iBAAiB,WAAW,CAAC,IAAZ,IAAoB,IAAI,CAAC,IAA1C;AAAA,SAA5B,CAAvB;AACA,aAAK,YAAL,CAAkB,MAAlB,CAAyB,gBAAzB,EAA2C,CAA3C;AACA,QAAA,IAAI,CAAC,QAAL,GAAgB,CAAhB;AACF;;AACA,MAAA,IAAI,CAAC,UAAL,GAAkB,CAAC,IAAI,CAAC,UAAxB;AACD,KAXM;AAYP,IAAA,aAZO,yBAYO,IAZP,EAYa,IAZb,EAYmB;AACxB,UAAI,IAAG,KAAM,KAAb,EAAoB;AAClB,QAAA,IAAI,CAAC,QAAL;AACF,OAFA,MAEO;AACL,YAAI,IAAI,CAAC,QAAL,GAAgB,CAApB,EAAuB;AACrB,UAAA,IAAI,CAAC,QAAL;AACF,SAFA,MAEO;AACN;AACA;AACC,UAAA,IAAI,CAAC,UAAL,GAAkB,KAAlB;AACF;AACF;AACD,KAxBM;AAyBP,IAAA,UAzBO,sBAyBI,IAzBJ,EAyBU;AACf;AACA;AACA,MAAA,IAAI,CAAC,UAAL,GAAkB,KAAlB;AACA,MAAA,IAAI,CAAC,QAAL,GAAgB,CAAhB;AACF;AA9BO;AArBI,CAAf","sourcesContent":["\n<template>\n  <div class=\"main-wrap\">\n    <div class=\"item-list\">\n      <div class=\"item\" v-for=\"item in itemList\" :key=\"item\" @click=\"updateCart(item)\" :class=\"{ selected: item.isSelected === true }\">\n        <div>{{ item.name }}</div>\n        <div>${{ item.amount }}</div>\n      </div>\n    </div>\n    <div class=\"cart\">\n      <div class=\"item\" v-for=\"item in cartNewOrder\" :key=\"item\">\n        <div class=\"item-wrap\" v-if=\"item.isSelected === true\">\n          <div>{{ item.name }}</div>\n          <div>@ &nbsp; {{ item.quantity }} x</div>\n          <div>${{ item.amount }}</div>\n          <div>${{ item.amount * item.quantity }}</div>\n          <div class=\"control-wrap\">\n            <div @click=\"updateItemQty('subract', item)\">-</div>\n            <div @click=\"updateItemQty('add', item)\">+</div>\n            <div @click=\"removeItem(item)\">x</div>\n          </div>\n        </div>\n      </div>\n      <div class=\"total\" v-if=\"calculateTotal > 0\">\n        <div>Total</div>\n        <div>${{ calculateTotal }}</div>\n      </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      itemList: [\n        { name: \"Milk\", amount: 10, isSelected: false, quantity: 0 }, // 0\n        { name: \"Fish\", amount: 20, isSelected: false, quantity: 0 }, // 1\n        { name: \"Lettuce\", amount: 30, isSelected: false, quantity: 0 }, // 2\n        { name: \"Rice\", amount: 5, isSelected: false, quantity: 0 } // 3\n      ],\n      cartNewOrder: []\n    }\n  },\n  computed: {\n    calculateTotal() {\n      let total = 0\n      for (const item of this.itemList) {\n        total = total + item.quantity * item.amount\n      }\n      return total\n    }\n  },\n  methods: {\n    updateCart(item) {\n      if (item.isSelected == false) {\n        this.cartNewOrder.unshift(item)\n        item.quantity++\n      } else {\n        let missingItemIndex = this.cartNewOrder.findIndex((missingItem) => missingItem.name == item.name)\n        this.cartNewOrder.splice(missingItemIndex, 1)\n        item.quantity = 0\n      }\n      item.isSelected = !item.isSelected\n    },\n    updateItemQty(mode, item) {\n      if (mode === \"add\") {\n        item.quantity++\n      } else {\n        if (item.quantity > 0) {\n          item.quantity--\n        } else {\n         //  let missingItemIndex = this.cartNewOrder.findIndex((missingItem) => missingItem.name == item.name)\n         //  this.cartNewOrder.splice(missingItemIndex, 1)\n          item.isSelected = false\n        }\n      }\n    },\n    removeItem(item) {\n      // let missingItemIndex = this.cartNewOrder.findIndex((missingItem) => missingItem.name == item.name)\n      // this.cartNewOrder.splice(missingItemIndex, 1)\n      item.isSelected = false\n      item.quantity = 0\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\nbody {\n  font-family: Source Sans Pro, sans-serif;\n}\n.main-wrap {\n  display: flex;\n  justify-content: space-between;\n  .item-list {\n    .item {\n      display: flex;\n      justify-content: space-between;\n      background-color: rgba(#ddd, 1);\n      margin: 0.5rem;\n      padding: 1rem;\n      border-radius: 0.5rem;\n      width: 10rem;\n      cursor: pointer;\n    }\n    .selected {\n      background-color: rgba(#eee, 1);\n      color: rgba(#a3a3a3, 1);\n    }\n  }\n  .cart {\n    .item {\n      display: flex;\n      width: 25rem;\n      .item-wrap {\n        display: flex;\n        flex: 1;\n        justify-content: space-between;\n        background-color: rgba(#ddd, 1);\n        margin: 0.25rem;\n        height: 1.5rem;\n        padding: 1rem;\n        border-radius: 0.5rem;\n        align-items: center;\n        &:hover {\n          .control-wrap {\n            display: flex;\n          }\n        }\n        .control-wrap {\n          display: none;\n          div {\n            padding: 0.5rem 1rem;\n            margin: 0 0.25rem;\n            background-color: rgba(#ff9200, 1);\n            border-radius: 0.5rem;\n            cursor: pointer;\n          }\n        }\n      }\n    }\n    .total {\n      display: flex;\n      justify-content: space-between;\n      background-color: rgba(#111, 1);\n      color: rgba(#fff, 1);\n      padding: 1rem;\n      margin: 0.25rem;\n      border-radius: 0.5rem;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}